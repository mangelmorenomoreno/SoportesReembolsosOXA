{
  "name": "soportereembolso",
  "version": "0.0.1",
  "description": "Build and deployment scripts",
  "scripts": {
    "clean": "rimraf --glob resources mta_archives mta-op*",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "undeploy": "cf undeploy soportereembolso --delete-services --delete-service-keys --delete-service-brokers"
  },
  "devDependencies": {
    "mbt": "^1.2.29",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "pdfmake": "^0.2.20"
  }
}
